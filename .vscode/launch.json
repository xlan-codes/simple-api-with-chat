    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Debug TypeScript in Node.js",
            "preLaunchTask": "npm: start:debug",
            "program": "${workspaceFolder}/src/main.ts",
            "cwd": "${workspaceFolder}",
            "protocol": "inspector",
            "outFiles": [
              "${workspaceFolder}/dist/**/*.js"
            ]
          }
    ]
}